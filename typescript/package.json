{
  "name": "@delorenj/gst",
  "version": "0.1.0",
  "description": "ðŸ¦‘ A power tool for quickly fetching, copying, and saving files from your GitHub Gists",
  "bin": {
    "gst": "./bin/run"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf lib",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "pnpm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2.8.0",
    "@oclif/plugin-help": "^5.0.0",
    "execa": "^7.0.0",
    "chalk": "^5.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.0",
    "eslint": "^8.0.0",
    "mocha": "^10.0.0",
    "oclif": "^3.0.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "oclif": {
    "bin": "gst",
    "dirname": "gst",
    "commands": "./lib/commands",
    "topicSeparator": " "
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "github",
    "gist",
    "cli",
    "fzf",
    "clipboard"
  ],
  "types": "lib/index.d.ts",
  "author": "delorenj",
  "license": "MIT",
  "homepage": "https://github.com/delorenj/gst#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delorenj/gst.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/delorenj/gst/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
